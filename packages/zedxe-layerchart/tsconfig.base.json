{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ESNext",
    "isolatedModules": true,
    "strict": true,
    "importsNotUsedAsValues": "error",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": false,
    "checkJs": false,
    "skipLibCheck": true,
    "types": ["svelte"],
    "verbatimModuleSyntax": true
  }
}
